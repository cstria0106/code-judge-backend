{
  "name": "@code-judge/api",
  "version": "0.1.13",
  "description": "SDK library generated by Nestia",
  "main": "lib/api/index.js",
  "typings": "lib/api/index.d.ts",
  "exports": {
    ".": {
      "default": "./lib/api/index.js",
      "types": "./lib/api/index.d.ts"
    }
  },
  "scripts": {
    "build": "npm run build:sdk && npm run build:lib",
    "build:sdk": "cd ../.. && rimraf ./src/api && npx nestia sdk && cd packages/api",
    "build:lib": "rimraf lib && tsc && find lib -type f ! -path \"lib/api/*\" | grep .js | xargs rm",
    "deploy": "npm run build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/samchon/nestia"
  },
  "author": "Jeongho Nam",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/samchon/nestia/issues"
  },
  "homepage": "https://nestia.io",
  "files": [
    "lib",
    "package.json",
    "README.md"
  ],
  "devDependencies": {
    "rimraf": "^5.0.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@nestia/fetcher": "^1.6.7"
  }
}
